{
	"$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"location": {
			"type": "string"
		}
	},
	"variables": {
	  "vnetLocation": "[parameters('location')]",
	  "storageAccountLocation": "[parameters('location')]"
	},
	"resources": [
	  {
		  "apiVersion": "2015-01-01",
		  "name": "vnet-template",
		  "type": "Microsoft.Resources/deployments",
		  "properties": {
			  "mode": "Incremental",
			  "templateLink": {
				  "uri": "[uri(deployment().properties.template.uri, 'Templates/VNET.json')]",
				  "contentVersion": "1.0.0.0"
			  },
			  "parameters": {
				  "vnetName": {
					  "value": "VNet"
				  },
				  "vnetAddressPrefix": {
					  "value": "10.0.0.0/16"
				  },
				  "subnet1Prefix": {
					  "value": "10.0.0.0/24"
				  },
				  "subnet1Name": {
					  "value": "Front-End"
				  },
				  "subnet2Prefix": {
					  "value": "10.0.1.0/24"
				  },
				  "subnet2Name": {
					  "value": "Back-End"
				  },
				  "vnetLocation": {
					  "value": "[variables('vnetLocation')]"
				  }
			  }
		  }
	  },
	  {
		  "apiVersion": "2015-01-01",
		  "name": "storageAccount-template",
		  "type": "Microsoft.Resources/deployments",
		  "properties": {
			  "mode": "Incremental",
			  "templateLink": {
				  "uri": "[uri(deployment().properties.template.uri, 'Templates/SA.json')]",
				  "contentVersion": "1.0.0.0"
			  },
			  "parameters": {
				  "storageAccountType": {
					  "value": "Standard_LRS"
				  },
				  "storageAccountLocation": {
					  "value": "[variables('storageAccountLocation')]"
				  },
				  "storageAccountName": {
					  "value": "azuretrainingsa1"
				  }
			  }
		  }
	  }
	]
}